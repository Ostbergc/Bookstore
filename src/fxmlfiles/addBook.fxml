<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.addBook">
   <children>
      <TextField fx:id="titletxt" layoutX="2.0" layoutY="66.0" prefHeight="45.0" prefWidth="352.0" promptText="title" />
      <TextField fx:id="genretxt" layoutX="5.0" layoutY="200.0" prefHeight="53.0" prefWidth="345.0" promptText="genre" />
      <TextField fx:id="quantitytxt" layoutX="413.0" layoutY="72.0" prefHeight="32.0" prefWidth="179.0" promptText="quantity" />
      <TextField fx:id="pricetxt" layoutX="413.0" layoutY="162.0" prefHeight="38.0" prefWidth="179.0" promptText="price" />
      <TextField fx:id="authortxt" layoutX="2.0" layoutY="126.0" prefHeight="45.0" prefWidth="352.0" promptText="author" />
      <Button fx:id="addbook" layoutY="293.0" mnemonicParsing="false" onAction="#add" prefHeight="65.0" prefWidth="220.0" text="Add" />
      <Button layoutX="428.0" layoutY="293.0" mnemonicParsing="false" onAction="#backButton" prefHeight="65.0" prefWidth="173.0" text="Back" />
   </children>
</AnchorPane>
